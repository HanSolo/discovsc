{
    "name": "discovsc",
    "displayName": "discovsc",
    "description": "",
    "version": "1.0.0",
    "publisher": "GerritGrunwald",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.discovsc"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.discovsc",
                "title": "Foojay JDK Discovery Service"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.30",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "node-fetch": "^2.6.1",
        "vscode-express": "^1.0.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HanSolo/discovsc"
    },
    "keywords": [
        "disco",
        "discoapi",
        "JDK",
        "OpenJDK",
        "Java"
    ],
    "author": "Gerrit Grunwald <ggrunwald@azul.com>",
